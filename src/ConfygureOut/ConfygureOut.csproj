<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <DebugType>full</DebugType>
    <Authors>imgen</Authors>
    <Company>C &amp; R Holdings LTD</Company>
    <Description>A configuration framework to mix configurations from different sources together</Description>
    <Copyright></Copyright>
    <PackageLicenseUrl>https://github.com/imgen/ConfygureOut/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/imgen/ConfygureOut</PackageProjectUrl>
    <RepositoryUrl>https://github.com/imgen/ConfygureOut/blob/master/LICENSE</RepositoryUrl>
    <PackageTags>configuration</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.1</Version>
    <PackageReleaseNotes>Better handling for the value that is not found in the configuration source</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="TupleExtensions.VictorGavrish" Version="1.2.4" />
  </ItemGroup>

</Project>
